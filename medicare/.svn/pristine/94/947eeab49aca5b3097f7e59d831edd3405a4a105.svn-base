<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%@taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
	<head>
		<title>定点区划</title>
		<c:set value="${pageContext.request.contextPath}" var="path" scope="page" />
		<link rel="stylesheet" href="${path}/easyui/themes/default/easyui.css" />
		<link rel="stylesheet" href="${path}/easyui/themes/icon.css" />
		<link rel="stylesheet" href="${path}/css/icon.css" />
		<link rel="stylesheet" href="${path }/css/ew-main.css" />
		<link rel="stylesheet" href="${path }/css/ew-home.css" />
		<script src="${path}/easyui/jquery-1.7.2.min.js"></script>
		<script src="${path}/easyui/jquery.easyui.min.js"></script>
		<script src="${path}/js/manager/listload.js"></script>
	</head>
	<body>
		<div id="content_wrap" class="easyui-layout content_wrap" data-options="fit:true" style="">
			<!-- 左侧药店列表  -->
			<div data-options="region:'west',title:'定点区划',split:true,width:200,minWidth:200,border:false" class="menu_warp" style="">
				<div class="easyui-list">
					<ul class="easyui-tree" data-options="lines:false">
						<li data-options="state:'closed', selected:true, checked:true " iconCls="icon-folder1">
							<span><a ghref="getAreaCus">全部</a></span>
							<ul>
								<li><span><a cusname="全部" ghref="getAreaCus?cusFlag=101">单体药店</a></span></li>
								<li><span><a cusname="全部" ghref="getAreaCus?cusFlag=102">连锁药店</a></span></li>
								<li><span><a cusname="全部" ghref="getAreaCus?cusFlag=201">门诊社区</a></span></li>
								<li><span><a cusname="全部" ghref="getAreaCus?cusFlag=301">医院</a></span></li>
							</ul>
						</li>
						<c:forEach items="${list}" var="list">
							<li data-options="state:'closed', selected:true, checked:true " iconCls="icon-folder1">
								<span><a ghref="getAreaCus?areacode=${list.acAreacode }">${list.acAreaname }</a></span>
								<ul>
									<li><span><a cusname="${list.acAreaname }" ghref="getAreaCus?cusFlag=101&areacode=${list.acAreacode }">单体药店</a></span></li>
									<li><span><a cusname="${list.acAreaname }" ghref="getAreaCus?cusFlag=102&areacode=${list.acAreacode }">连锁药店</a></span></li>
									<li><span><a cusname="${list.acAreaname }" ghref="getAreaCus?cusFlag=201&areacode=${list.acAreacode }">门诊社区</a></span></li>
									<li><span><a cusname="${list.acAreaname }" ghref="getAreaCus?cusFlag=301&areacode=${list.acAreacode }">医院</a></span></li>
								</ul>
							</li>
						</c:forEach>
						<li data-options="state:'closed', selected:true, checked:true " iconCls="icon-folder1">
							<span><a ghref="getAreaCus?areacode=0">未划分地区</a></span>
							<ul>
								<li><span><a cusname="未划分地区" ghref="getAreaCus?areacode=0&cusFlag=101">单体药店</a></span></li>
								<li><span><a cusname="未划分地区" ghref="getAreaCus?areacode=0&cusFlag=102">连锁药店</a></span></li>
								<li><span><a cusname="未划分地区" ghref="getAreaCus?areacode=0&cusFlag=201">门诊社区</a></span></li>
								<li><span><a cusname="未划分地区" ghref="getAreaCus?areacode=0&cusFlag=301">医院</a></span></li>
							</ul>
						</li>
					</ul>
				</div>
			</div>
			<!-- 中间药店内容 -->
			<div id="mainPanle" region="center" style="overflow: auto">
				<div id="tabs" class="easyui-tabs" fit="true" border="false">

					<div id="mm" class="easyui-menu"
						style="width: 150px; overflow: scroll">
						<div id="mm-tabreload">
							刷新
						</div>
						<div id="mm-tabcloseall">
							关闭全部
						</div>
						<div id="mm-tabcloseother">
							关闭其他
						</div>
						<div class="menu-sep"></div>
						<div id="mm-tabcloseright">
							关闭右侧标签
						</div>
						<div id="mm-tabcloseleft">
							关闭左侧标签
						</div>
					</div>
				</div>
			</div>
		</div>
	</body>
</html>
