	background-image: linear-gradient(to bottom,#fff,#BFDBFF,#fff);
	position: absolute;
	bottom: 0;
	left: 0;
}

.right_grxl_header {
	width: 100%;
	height: 24px;
	background: #E0ECFF;
	position: absolute;
	top: 0;
	left: 0;
	border-bottom: 1px #ccc solid;
}

.right_grxl_header p {
	line-height: 24px;
	margin-left: 8px;
}

.right_grxl_header img {
	padding: 4px 8px;
	float: left;
}

.right_grxl:hover {
	border-top: 4px red solid;
	background: #FFE48D;
	box-shadow: 2px 2px 8px #333;
	background-image: linear-gradient(to bottom,#fff,#BFDBFF,#BFDBFF,#fff);
}

.right_grxl ul li {
	padding: 8px 10px;
	color: #333;
}

.right_grxl ul li:nth-child(1) {
	margin-top: 8px;
}

#time {
	width: 140px;
	height: 16px;
	margin-top: 10px;
}

.right_MMbtn {
	width: 90%;
	height: 26px;
	border: 1px cornflowerblue solid;
	margin: 8px auto;
	text-align: center;
	border-radius: 2px;
}

.right_MMbtn a {
	display: block;
	color: black;
	text-decoration: none;
	line-height: 26px;
	font-family: "微软雅黑";
}

.right_MMbtn a:hover {
	border: 1px dodgerblue solid;
	border-radius: 2px;
}

.right_TC {
	width: 90%;
	height: 26px;
	border: 1px cornflowerblue solid;
	margin: 8px auto;
	text-align: center;
	border-radius: 2px;
}

.right_TC a {
	display: block;
	color: black;
	text-decoration: none;
	line-height: 26px;
	font-family: "微软雅黑";
	transition: 0.1s linear;
}

.right_TC a:hover {
	border-radius: 2px;
	background: #E3170D;
	color: #fff;
}


/*=============================================上传预警表格*/
p,
table, caption, td, tr, th {
	margin: 0;
	padding: 0;
	font-weight: normal;
}

p {
	margin-bottom: 15px;
}

table {
	border-collapse: collapse;
	height: 92%;
	width: 99%;
	margin: 30px auto;
	text-align: center;
	font-size: 12px;
}

table td,
table th {
	padding: 5px;
	border: 1px solid #fff;
	border-width: 0 1px 1px 0;
}

thead th {
	background:#f5f5f5;
}

thead th[colspan],
thead th[rowspan] {
	background: #fff;
	/*表格头*/
	height: 40px;
	width: 150px;
}

tbody th,
	tfoot th {
	text-align: center;
	background: #f5f5f5;
		/*表格左侧列*/
	height: 30px;
}

tbody td,
	tfoot td {
	text-align: center;
	background: #fff;
}

tbody tr.odd td {
	background: #f5f5f5;
}

.red {
	color: red;
}
