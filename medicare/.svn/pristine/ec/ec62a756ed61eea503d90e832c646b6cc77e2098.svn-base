.mmxg_box {
	width: 1200px;
	height: 610px;
	border: 1px #FD9940 solid;
	margin: 2% auto;
	border-top: 5px #FD9940 solid;
	border-left: none;
	border-right: none;
	border-radius: 4px;
}

.mmxg_header {
	width: 100%;
	height: 60px;
	background: #fff;
	border-radius: 2px;
}

.header-logo img {
	margin-top: 15px;
}

.mmxg_box .mmxg-content {
	width: 1200px;
	height: 550px;
	background: #fbfbfb;
}

.mmxg_box .mmxg-content .centerr {
	width: 500px;
	height: 290px;
	margin: 0 auto;
	position: relative;
}

.mmxg_box .mmxg-content .centerr .content-text-top {
	position: absolute;
	bottom: 60px;
	left: 0;
	width: 500px;
	height: 30px;
	text-align: center;
}

.mmxg_box .mmxg-content .centerr .content-text-top img {
	margin-left: 42px;
}

.mmxg_box .mmxg-content .centerr .content-text-bottom {
	position: absolute;
	bottom: 0;
	left: 0;
	width: 500px;
	height: 30px;
	text-align: center;
}

